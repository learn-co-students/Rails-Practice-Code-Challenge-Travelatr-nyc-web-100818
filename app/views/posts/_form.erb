<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br><br>
  <%= f.label :content %>
  <%= f.text_area :content %>
    <br><br>
  <%= f.label :likes %>
  <%= f.number_field :likes %>
    <br><br>
  <%= f.collection_select :blogger_id, @bloggers, :id, :name %>
  <br><br>
  <%= f.collection_select :destination_id, @destinations, :id, :name %>
  <br><br>
  <%= f.submit %>
<% end %>
